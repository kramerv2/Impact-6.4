                        alt="{{ image.alt }}" 
                        {% if lazy %}
                        loading="lazy"
                        {% endif %}
                    >
                {% else %}
                    {{ 'image' |  placeholder_svg_tag }}
                {% endif %}

                {% if image_shape == "1" %}
